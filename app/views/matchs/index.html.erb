<div class="container">
  <div class="row">
    <div class="col col-xs-12 col-lg-10 offset-lg-1">
      <h1 style="text-align: center">Planning des Matchs</h1>
    </div>
  </div>
  <div class="row" id="matchs-list">
    <div class="col col-xs-6 col-lg-10 offset-lg-1">
      <%= month_calendar number_of_weeks: 3 do |date| %>
        <%= link_to date.day, match_path(date) %>
      <% end %>
    </div>
  </div>
</div>
